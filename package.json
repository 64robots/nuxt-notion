{
  "name": "nuxt-notion",
  "version": "0.0.1",
  "description": "Nuxt Module for Notion API",
  "main": "lib/module.js",
  "scripts": {
    "dev": "nuxt test/fixture",
    "lint": "eslint lib test",
    "test": "npm run lint && jest",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "dependencies": {
    "@notionhq/client": "^0.1.3",
    "npm-publish-git-tag": "^3.0.3"
  },
  "devDependencies": {
    "codecov": "3.7.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "jest": "^25.1.0",
    "nuxt": "^2.14.9",
    "standard-version": "^8.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/64robots/nuxt-notion.git"
  },
  "keywords": [
    "nuxt",
    "notion"
  ],
  "author": "64 Robots",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/64robots/nuxt-notion/issues"
  },
  "homepage": "https://github.com/64robots/nuxt-notion#readme",
  "publishConfig": {
    "access": "public"
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "files": [
    "lib",
    "*.d.ts"
  ],
  "types": [
    "index.d.ts"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  }
}
